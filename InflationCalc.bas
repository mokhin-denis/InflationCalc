    Do While DateDiff("m", дата, дата_конец) >= 0
        результат = результат * (1# + ТаблицаИнфляции(Year(дата), Month(дата)) / 100#)
        дата = DateAdd("m", 1, дата)
    Loop
    
    ИнфляцияЗаПериод = результат - 1#
End Function